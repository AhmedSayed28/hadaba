function WebMethodGET(url, onSuccess, onError) {
    serviceFailure = false;
    $.ajax({
        type: "GET",
        url: url,
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        async: false,
        success: onSuccess,
        error: onError
    });
}

function WebMethodPOST(url, posteddata, onSuccess, onError) {
    serviceFailure = false;
    return $.ajax({
        type: "POST",
        url: url,
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        sync: true,
        data: JSON.stringify(posteddata),
        success: onSuccess,
        error: onError
    });
}

